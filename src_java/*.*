package org.javaro.lecture;

public class Ex {
	public static void main(String[] args) {
		System.out.println("김지훈" + "-" + "20151623" + "-" + "변수");
		int grade = 3;
		System.out.println(grade + "학년" + "홍길동");
		System.out.println(grade + "학년" + "이몽룡");
		System.out.println(grade + "학년" + "성춘향");
		System.out.println(grade + "학년" + "변학도");
	}
}
